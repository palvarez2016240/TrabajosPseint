Proceso TablaDeMultiplicarM
	
	Escribir "Que tabla desea"
	leer a
	
	Escribir "Hasta que numero"
	leer b
	
	c <- 0
	
	Mientras b!=c Hacer
		
		c = c+1
		t = a*c
		
		escribir a "*" c "=" t
		
		
	Fin Mientras
	
	Escribir "Desea agregar otra tabla oprima 1"
	leer d
	
	Mientras d!=1 
		
	Fin Mientras

FinProceso
